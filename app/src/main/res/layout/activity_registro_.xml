<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".Registro_Activity">


    <FrameLayout
        android:id="@+id/frameLayout10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="391dp"
            android:layout_height="169dp"
            android:contentDescription="@string/imagen_usuario"
            tools:srcCompat="@tools:sample/avatars" />
    </FrameLayout>



    <TextView
        android:id="@+id/textView5"
        style="@style/titulos"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="@string/rfc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout10" />


    <EditText
        android:id="@+id/textRfc"
        style="@style/cajasTexto"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:hint="@string/ingrese_su_rfc"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout10"
        tools:ignore="TextFields"
        android:importantForAutofill="no" tools:targetApi="o" />


    <ImageButton
        android:id="@+id/btnBuscar"
        android:layout_width="48dp"
        android:layout_height="43dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/buscador_curp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textRfc"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout10"
        app:srcCompat="@android:drawable/ic_menu_search"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textNombre"
        style="@style/titulos"
        android:layout_marginStart="20dp"
        android:layout_marginTop="35dp"
        android:text="@string/nombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/tv_nombre"
        style="@style/texto"
        android:layout_width="200dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/textNombre"
        app:layout_constraintTop_toBottomOf="@+id/textRfc" />

    <TextView
        android:id="@+id/textPaterno"
        style="@style/titulos"
        android:layout_marginStart="20dp"
        android:layout_marginTop="35dp"
        android:text="@string/apellido_paterno"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNombre"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_ap"
        style="@style/texto"
        android:layout_width="200dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/textPaterno"
        app:layout_constraintTop_toBottomOf="@+id/tv_nombre" />


    <TextView
        android:id="@+id/textMaterno"
        style="@style/titulos"
        android:layout_marginStart="20dp"
        android:layout_marginTop="35dp"
        android:text="@string/apellido_materno"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textPaterno" />

    <TextView
        android:id="@+id/tv_am"
        style="@style/texto"
        android:layout_width="200dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toEndOf="@+id/textMaterno"
        app:layout_constraintTop_toBottomOf="@+id/tv_ap" />


    <TextView
        android:id="@+id/textView7"
        style="@style/titulos"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="@string/contrase_a"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textMaterno" />


    <EditText
        android:id="@+id/txtContraseña"
        style="@style/cajasTexto"
        android:layout_width="243dp"
        android:layout_height="46dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:hint="@string/ingrese_su_contrase_a"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/tv_am"
        android:inputType=""
        android:importantForAutofill="no" tools:targetApi="o" />

    <FrameLayout
        android:id="@+id/frameLayout12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:paddingBottom="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtContraseña">

        <Button
            android:id="@+id/btn_registrarse"
            style="@style/style_boton_confirmar"
            android:layout_gravity="center"
            android:foregroundGravity="center"
            android:text="@string/registrarse" />
    </FrameLayout>















</androidx.constraintlayout.widget.ConstraintLayout>